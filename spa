        price: 119
      }
      ,{
        service: "Footspa",
        duration: "30 mins",
        price: 299
      }
    ];

    // Function to format the price (range or single)
    function formatPrice(service) {
      if (service.priceRange) {
        return `₱${service.priceRange.min.toLocaleString()}–₱${service.priceRange.max.toLocaleString()} (${service.notes})`;
      }
      return `₱${service.price.toLocaleString()}`;
    }

    // Populate products
    const productList = document.getElementById('productList');
    products.forEach(prod => {
      const col = document.createElement('div');
      col.className = 'col-md-3 col-sm-6 mb-4';
      col.innerHTML = `
        <div class="product-card h-100 d-flex flex-column">
          <img src="${prod.image}" alt="${prod.name}" class="product-image" />
          <div class="product-body flex-grow-1 d-flex flex-column justify-content-between">
            <div>
              <h5 class="product-title">${prod.name}</h5>
              <div class="text-warning mb-2">
                ${'<i class="fas fa-star"></i>'.repeat(prod.rating)}
                ${prod.rating % 1 !== 0 ? '<i class="fas fa-star-half-alt"></i>' : ''}
              </div>
              <div class="product-price">₱${prod.price}</div>
            </div>
        
          </div>
        </div>`;
      productList.appendChild(col);
    });

    // Populate pricing table
    const pricingTable = document.getElementById('pricingTable');
    pricing.forEach((service, index) => {
      const row = document.createElement('tr');
      row.innerHTML = `
        <td>${index + 1}</td>
        <td>${service.service}</td>
        <td>${service.duration}</td>
        <td>${formatPrice(service)}</td>`;
      pricingTable.appendChild(row);
    });
  </script>

